
.nav{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
}
.goBack{
	width: 80px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.goBack::before{
	content: '';
	width: 10px;
	height: 10px;
	border-left: 1px solid #000;
	border-bottom: 1px solid #000;
	-webkit-transform: rotate(45deg);
	        transform: rotate(45deg);
}
.navigator {
	-webkit-box-flex:1;
	-webkit-flex:1;
	        flex:1;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	background: #fff;
	font-size: 30rpx;
	font-weight: 700;
	padding-left: 23%;
}
.container {
	margin-top: 30px;
	padding: 10px;
}
.orderState {
	font-size: 20px;
	font-weight: 600;
	margin-left: 10px;
}
.orderDeatil {
	margin-top: 20px;
	margin-left: 10px;
}
.ordertitle {
	background: url(data:image/png;base64,) no-repeat left center;
	background-size: 70rpx;
	padding-left: 45rpx;
	font-size: 30rpx;
	font-weight: 600;
	vertical-align: middle;
}
.orderPanel {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	font-size: 15px;
	padding-top: 10px;
	line-height: 40px;
	border-bottom: 1px solid #C8C7CC;
}
.orderFood {
	width: 60%;
}
.orderPrice {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.orderAmount {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.totalPanel {
	line-height: 100rpx;
	font-size: 35rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: reverse;
	-webkit-flex-direction: row-reverse;
	        flex-direction: row-reverse;
	border-bottom: 1px solid #C8C7CC;
}
.total {
	margin-right: 20rpx;
	-webkit-background-clip: text;
	-webkit-text-fill-color: transparent;
	background-image: -webkit-linear-gradient(left, #ff9500, #FBCA18);
	background-image: linear-gradient(to right, #ff9500, #FBCA18);
}
.remarkPanel {
	background: url(data:image/png;base64,) no-repeat left center;
	background-size: 38rpx;
	font-size: 30rpx;
	font-weight: 600;
	padding-left: 45rpx;
	margin: 20px 10px 10px 10px;
}
.van-button--normal {
	font-size: 18px !important;
}
..van-goods-action-button__inner {
	height: 45px !important;
}

